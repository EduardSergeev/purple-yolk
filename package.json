{
  "//": "https://code.visualstudio.com/api/references/extension-manifest",
  "activationEvents": [
    "onLanguage:haskell"
  ],
  "badges": [
    {
      "description": "Build status",
      "href": "https://travis-ci.org/tfausak/purple-yolk",
      "url": "https://travis-ci.org/tfausak/purple-yolk.svg?branch=master"
    }
  ],
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "configuration": {
      "properties": {
        "purpleYolk.ghci.command": {
          "default": "stack ghci",
          "description": "The command to start GHCi.",
          "pattern": "\\S",
          "type": "string"
        }
      },
      "title": "Purple Yolk"
    },
    "grammars": [
      {
        "language": "cabal",
        "path": "grammars/cabal-grammar.json",
        "scopeName": "source.cabal"
      },
      {
        "language": "haskell",
        "path": "grammars/haskell-grammar.json",
        "scopeName": "source.haskell"
      }
    ],
    "languages": [
      {
        "aliases": [
          "Cabal"
        ],
        "configuration": "languages/cabal-language.json",
        "extensions": [
          ".cabal"
        ],
        "id": "cabal"
      },
      {
        "aliases": [
          "Haskell"
        ],
        "configuration": "languages/haskell-language.json",
        "extensions": [
          ".hs"
        ],
        "id": "haskell"
      }
    ],
    "snippets": [
      {
        "language": "cabal",
        "path": "snippets/cabal-snippets.json"
      },
      {
        "language": "haskell",
        "path": "snippets/haskell-snippets.json"
      }
    ]
  },
  "dependencies": {
    "vscode-languageclient": "^6.1.1",
    "vscode-languageserver": "^6.1.1"
  },
  "description": "A Haskell IDE.",
  "devDependencies": {
    "eslint-loader": "^3.0.3",
    "eslint": "^6.8.0",
    "purescript": "^0.13.6",
    "purs-loader": "^3.7.1",
    "vsce": "^1.74.0",
    "watchy": "^0.9.7",
    "webpack-cli": "^3.3.11",
    "webpack": "^4.42.0"
  },
  "displayName": "Purple Yolk",
  "keywords": [
    "haskell"
  ],
  "engines": {
    "node": "^13.11.0",
    "npm": "^6.13.7",
    "vscode": "^1.43.0"
  },
  "homepage": "https://github.com/tfausak/purple-yolk",
  "icon": "images/purple-yolk.png",
  "license": "ISC",
  "main": "dist/client.js",
  "name": "purple-yolk",
  "publisher": "taylorfausak",
  "repository": {
    "type": "git",
    "url": "https://github.com/tfausak/purple-yolk.git"
  },
  "scripts": {
    "eslint": "eslint",
    "test": "webpack -d && node dist/test.js",
    "purs": "purs",
    "vsce": "vsce",
    "vscode:prepublish": "webpack -p && node dist/test.js",
    "watchy": "watchy",
    "webpack": "webpack"
  },
  "version": "0.0.5"
}
